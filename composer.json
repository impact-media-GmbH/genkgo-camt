{
    "name": "genkgo/camt",
    "description": "Library to read CAMT files (XML containing bank statements).",
    "license": "MIT",
    "scripts": {
        "check": [
            "php-cs-fixer fix --ansi --dry-run --diff",
            "phpunit --color=always",
            "phpstan analyse --ansi"
        ],
        "fix": [
            "php-cs-fixer fix --ansi"
        ]
    },
    "require": {
        "php": "~5.6 || ~7.4",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "moneyphp/money": "^3.3 || ^4.0",
        "symfony/options-resolver": "^3.4.47",
        "symfony/yaml": "^3.4.47"
    },
    "autoload": {
        "psr-4": {
            "Genkgo\\Camt\\": "src",
            "Iban\\Validation\\": "iban/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Genkgo\\TestCamt\\": "test",
            "Iban\\Validation\\Tests\\": "iban/Tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27"
    }
}
